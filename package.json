{
  "name": "dyal",
  "version": "0.0.3",
  "repository": "https://github.com/lstrojny/dyal",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "rollup-plugin-uglify": "^6.0.0",
    "typescript": "^3.1.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "scripts": {
    "build": "yarn && rollup -c",
    "dev": "rollup -c --watch",
    "fmt": "prettier --write src/*",
    "testbed": "rollup -c --file=testbed/dist/loader.js && cd testbed && python -m SimpleHTTPServer 8081",
    "publish": "yarn && rollup -c && npm publish"
  },
  "dependencies": {
    "rollup-plugin-typescript": "^1.0.0",
    "tslib": "^1.9.3"
  }
}
